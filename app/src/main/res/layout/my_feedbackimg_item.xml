<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".view.activity.OptionReturnActivity" android:background="@color/white">
    <FrameLayout
        android:layout_width="wrap_content"  android:id="@+id/head_fm_id"
        android:layout_height="wrap_content" android:layout_marginBottom="5dp">
        <ImageView
            android:layout_width="70dp" android:id="@+id/upload_img"
            android:layout_height="70dp" android:layout_marginTop="7dp"
            android:scaleType="fitXY"/>
        <ImageView
            android:layout_width="17dp" android:id="@+id/close_id"
            android:layout_height="17dp" android:src="@mipmap/circle_delete"
            android:layout_marginLeft="58dp"/>
    </FrameLayout>

</RelativeLayout>