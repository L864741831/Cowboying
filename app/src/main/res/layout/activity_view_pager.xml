<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rvs_id"  android:background="@color/colorAccent"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <ImageView
        android:layout_width="35dp" android:id="@+id/back_id"
        android:layout_height="30dp" android:src="@mipmap/back"
        android:scaleType="fitXY"
        android:layout_marginLeft="15dp"/>
    <com.ibeef.cowboying.view.customview.HackyViewPager
        android:id="@+id/view_pager"
        android:layout_width="match_parent"
        android:layout_height="270dp" android:layout_centerInParent="true"/>
</RelativeLayout>